<?xml version="1.0"?>
<clientConfig version="1.2">
  <emailProvider id="solcon.nl">
    <domain>solcon.nl</domain>
    <displayName>Solcon</displayName>
    <displayShortName>Solcon</displayShortName>
    <incomingServer type="imap">
      <hostname>imap.solcon.nl</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="imap">
      <hostname>imap.solcon.nl</hostname>
      <port>143</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="pop3">
      <hostname>pop.solcon.nl</hostname>
      <port>995</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="pop3">
      <hostname>pop.solcon.nl</hostname>
      <port>110</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <outgoingServer type="smtp">
      <hostname>mail.solcon.nl</hostname>
      <port>587</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </outgoingServer>
    <documentation url="https://www.solcon.nl/klantenservice/?mt=internet-wifi&amp;st=installeren-instellen&amp;pq=e-mail-instellen&amp;sq=algemene-e-mail-instellingen-smtp-imap-pop&amp;tq=imap#"/>
    <documentation url="https://www.solcon.nl/klantenservice/?mt=internet-wifi&amp;st=installeren-instellen&amp;pq=e-mail-instellen&amp;sq=algemene-e-mail-instellingen-smtp-imap-pop&amp;tq=pop3#"/>
  </emailProvider>
</clientConfig>