<?xml version="1.0"?>
<clientConfig version="1.2">
  <emailProvider id="ozonline.com.au">
    <domain>ozonline.com.au</domain>
    <displayName>Australia Online</displayName>
    <displayShortName>Australia Online</displayShortName>
    <incomingServer type="imap">
      <hostname>imap.ozonline.com.au</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="imap">
      <hostname>imap.ozonline.com.au</hostname>
      <port>143</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="pop3">
      <hostname>pop.ozonline.com.au</hostname>
      <port>995</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="pop3">
      <hostname>pop.ozonline.com.au</hostname>
      <port>110</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <outgoingServer type="smtp">
      <hostname>smtp.ozonline.com.au</hostname>
      <port>465</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </outgoingServer>
    <outgoingServer type="smtp">
      <hostname>smtp.ozonline.com.au</hostname>
      <port>587</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </outgoingServer>
    <documentation url="https://www.australiaonline.au/help/email.html"/>
    <!--IMAP Port: encrypted 993, plain 143
POP Port: encrypted 995, plain 110
SMTP: encrypted 465, plain 587-->
  </emailProvider>
</clientConfig>