<?xml version="1.0"?>
<clientConfig version="1.2">
  <emailProvider id="cablelink.at">
    <domain>cablelink.at</domain>
    <domain>sbg.at</domain>
    <displayName>Salzburg AG</displayName>
    <displayShortName>Salzburg AG</displayShortName>
    <incomingServer type="imap">
      <hostname>mail.cablelink.at</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="imap">
      <hostname>mail.cablelink.at</hostname>
      <port>143</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="pop3">
      <hostname>mail.cablelink.at</hostname>
      <port>995</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="pop3">
      <hostname>mail.cablelink.at</hostname>
      <port>110</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <outgoingServer type="smtp">
      <hostname>smtp.cablelink.at</hostname>
      <port>587</port>
      <socketType>plain</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </outgoingServer>
    <documentation url="https://www.salzburg-ag.at/service/internet-anleitungen/e-mail.html"/>
  </emailProvider>
</clientConfig>