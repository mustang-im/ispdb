<?xml version="1.0"?>
<clientConfig version="1.2">
  <emailProvider id="hccnet.nl">
    <domain>hccnet.nl</domain>
    <displayName>HCC</displayName>
    <displayShortName>HCC</displayShortName>
    <incomingServer type="imap">
      <hostname>imap.hccnet.nl</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="pop3">
      <hostname>pop3.hccnet.nl</hostname>
      <port>995</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <outgoingServer type="smtp">
      <hostname>smtp.hccnet.nl</hostname>
      <port>587</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </outgoingServer>
    <documentation url="https://hcc.nl/kennis/kennis/nieuws/hcc-net-mail-update-en-oplossingen"/>
  </emailProvider>
</clientConfig>