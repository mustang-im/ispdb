<?xml version="1.0"?>
<clientConfig version="1.2">
  <emailProvider id="stofanet.dk">
    <domain>stofanet.dk</domain>
    <domain>stofa.dk</domain>
    <domain>mailnorlys.dk</domain>
    <domain>norlys.dk</domain>
    <displayName>Norlys</displayName>
    <displayShortName>Norlys</displayShortName>
    <incomingServer type="imap">
      <hostname>mail.mailnorlys.dk</hostname>
      <port>143</port>
      <socketType>plain</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
