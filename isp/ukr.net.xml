    <displayName>UKR.NET</displayName>
    <displayShortName>UKR.NET</displayShortName>
    <incomingServer type="imap">
      <hostname>imap.ukr.net.</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <username/>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="imap">
      <hostname>imap.ukr.net.</hostname>
      <port>143</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
