<?xml version="1.0"?>
<clientConfig version="1.2">
  <emailProvider id="online.nl">
    <domain>online.nl</domain>
    <displayName>Online.nl</displayName>
    <displayShortName>Online.nl</displayShortName>
    <incomingServer type="imap">
      <hostname>imap.online.nl</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <username/>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="imap">
      <hostname>imap.online.nl</hostname>
      <port>143</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <outgoingServer type="smtp">
      <hostname>smtp.online.nl</hostname>
      <port>465</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </outgoingServer>
    <outgoingServer type="smtp">
      <hostname>smtp.online.nl</hostname>
      <port>587</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </outgoingServer>
  </emailProvider>
</clientConfig>