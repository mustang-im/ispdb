<?xml version="1.0"?>
<clientConfig version="1.2">
  <emailProvider id="netcologne.de">
    <domain>netcologne.de</domain>
    <displayName>NetCologne</displayName>
    <displayShortName>NetCologne</displayShortName>
    <incomingServer type="imap">
      <hostname>imap.netcologne.de</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="imap">
      <hostname>imap.netcologne.de</hostname>
      <port>143</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="pop3">
      <hostname>pop3.netcologne.de</hostname>
      <port>995</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="pop3">
      <hostname>pop3.netcologne.de</hostname>
      <port>110</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <outgoingServer type="smtp">
      <hostname>smtp.netcologne.de</hostname>
      <port>465</port>
      <socketType>SSL</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </outgoingServer>
    <documentation url="https://www.netcologne.de/privatkunden/faq/mail-cloud/e-mail-einstellungen/wo-finde-ich-die-einstellungen-fuer-pop3-imap-und-smtp/?page=1"/>
  </emailProvider>
</clientConfig>