      <hostname>mail.zeelandnet.nl</hostname>
      <port>587</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </outgoingServer>
    <documentation url="https://www.delta.nl/klantenservice/internet-en-wifi/e-mail/serverinstellingen/"/>
  </emailProvider>
</clientConfig>