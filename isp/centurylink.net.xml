<?xml version="1.0"?>
<clientConfig version="1.2">
  <emailProvider id="centurylink.net">
    <domain>centurylink.net</domain>
    <domain>centurytel.net</domain>
    <domain>clds.net</domain>
    <domain>coastalnow.net</domain>
    <domain>cochill.net</domain>
    <domain>cswnet.com</domain>
    <domain>elpasotel.net</domain>
    <domain>embarqmail.com</domain>
    <domain>emadisonriver.com</domain>
    <domain>emadisonriver.net</domain>
    <domain>gallatinriver.net</domain>
    <domain>grics.net</domain>
    <domain>gulftel.com</domain>
    <domain>madisonriver.biz</domain>
    <domain>mebtel.net</domain>
    <domain>qwest.net</domain>
    <domain>q.com</domain>
    <displayName>CenturyLink</displayName>
    <displayShortName>CenturyLink</displayShortName>
    <incomingServer type="imap">
      <hostname>mail.centurylink.net</hostname>
      <port>143</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <incomingServer type="pop3">
      <hostname>pop.centurylink.net</hostname>
      <port>110</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </incomingServer>
    <outgoingServer type="smtp">
      <hostname>smtp.centurylink.net</hostname>
      <port>587</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </outgoingServer>
    <outgoingServer type="smtp">
      <hostname>smtp.centurylink.net</hostname>
      <port>25</port>
      <socketType>STARTTLS</socketType>
      <username>%EMAILADDRESS%</username>
      <authentication>password-cleartext</authentication>
    </outgoingServer>
  </emailProvider>
</clientConfig>