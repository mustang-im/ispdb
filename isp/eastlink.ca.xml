    <displayName>Eastlink</displayName>
    <displayShortName>Eastlink</displayShortName>
    <incomingServer type="imap">
      <hostname>mail.eastlink.ca</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <username/>
      <authentication>password-cleartext</authentication>
